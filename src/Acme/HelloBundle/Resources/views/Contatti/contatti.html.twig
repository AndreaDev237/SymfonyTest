{% extends 'AcmeHelloBundle::layout.html.twig' %}
{% set active = 'contatti' %}
 {% block body %}
<b>Contattaci!</b>
<form action="{{ path('contatti') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}   
    {{ form_row(form.email) }}
    {{ form_row(form.nome) }}
    {{ form_rest(form) }}
      
    <input type="submit" />
</form>
 {% endblock %}